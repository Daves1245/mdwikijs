{
  "name": "mdwikijs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:local": "WIKI_ROOT_PATH=./wiki next dev --turbopack",
    "dev:node": "npm run dev",
    "dev:node:local": "WIKI_ROOT_PATH=./wiki npm run dev",
    "build": "next build",
    "build:node": "npm run build",
    "start": "next start",
    "start:node": "npm start",
    "lint": "next lint",
    "install:node": "npm install",
    "install:bun": "bun install",
    "docker:build": "docker build -t mdwikijs .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rehype-stringify": "^10.0.1",
    "rehype-toc": "^3.0.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-toc": "^9.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/chokidar": "^2.1.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
